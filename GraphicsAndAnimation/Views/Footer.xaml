<UserControl
    x:Class="GraphicsAndAnimation.Views.Footer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModel="clr-namespace:GraphicsAndAnimation.ViewModels"
    d:DesignHeight="100"
    d:DesignWidth="525"
    mc:Ignorable="d">
    <UserControl.DataContext>
        <viewModel:FooterViewModel />
    </UserControl.DataContext>
    <UserControl.Resources>
        <ResourceDictionary Source="Resources.xaml" />
    </UserControl.Resources>
    <Grid>
        <Grid.Background>
            <StaticResource ResourceKey="FotterBackgroundBrush" />
        </Grid.Background>
        <ContentControl Content="{StaticResource Sheep}" Focusable="False" />
    </Grid>
</UserControl>
